{
  "name": "@ivxp/sdk",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "IVXP SDK - Client library for the Intelligence Value Exchange Protocol",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@ivxp/protocol": "workspace:*",
    "viem": "^2.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@ivxp/test-utils": "workspace:*",
    "tsup": "^8.0.0"
  },
  "license": "MIT"
}
