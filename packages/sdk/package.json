{
  "name": "@ivxp/sdk",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "TypeScript SDK for IVXP Protocol - Client library for the Intelligence Value Exchange Protocol",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./crypto": {
      "types": "./dist/crypto/index.d.ts",
      "import": "./dist/crypto/index.js",
      "require": "./dist/crypto/index.cjs"
    },
    "./payment": {
      "types": "./dist/payment/index.d.ts",
      "import": "./dist/payment/index.js",
      "require": "./dist/payment/index.cjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.cjs"
    },
    "./errors": {
      "types": "./dist/errors/index.d.ts",
      "import": "./dist/errors/index.js",
      "require": "./dist/errors/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:interop": "vitest run --reporter=verbose src/__tests__/interop/",
    "test:interop:ts-ts": "vitest run --reporter=verbose --reporter=junit --outputFile.junit=test-results/junit.xml src/__tests__/interop/ts-ts.test.ts"
  },
  "dependencies": {
    "@ivxp/protocol": "workspace:*",
    "viem": "^2.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@ivxp/test-utils": "workspace:*",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "keywords": [
    "ivxp",
    "protocol",
    "sdk",
    "web3",
    "usdc",
    "agent",
    "base",
    "eip-191",
    "crypto",
    "payment"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/franksprotocols/ivxp-protocol.git",
    "directory": "packages/sdk"
  },
  "license": "MIT"
}
