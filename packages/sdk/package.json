{
  "name": "@ivxp/sdk",
  "version": "0.1.0",
  "type": "module",
  "description": "TypeScript SDK for IVXP Protocol - Client library for the Intelligence Value Exchange Protocol",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./crypto": {
      "types": "./dist/crypto/index.d.ts",
      "import": "./dist/crypto/index.js",
      "require": "./dist/crypto/index.cjs"
    },
    "./payment": {
      "types": "./dist/payment/index.d.ts",
      "import": "./dist/payment/index.js",
      "require": "./dist/payment/index.cjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.cjs"
    },
    "./errors": {
      "types": "./dist/errors/index.d.ts",
      "import": "./dist/errors/index.js",
      "require": "./dist/errors/index.cjs"
    },
    "./provider": {
      "types": "./dist/provider/index.d.ts",
      "import": "./dist/provider/index.js",
      "require": "./dist/provider/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:interop": "vitest run --config vitest.interop.config.ts --reporter=verbose src/__tests__/interop/",
    "test:interop:ts-ts": "vitest run --config vitest.interop.config.ts --reporter=verbose --reporter=junit --outputFile.junit=test-results/junit.xml src/__tests__/interop/ts-ts.test.ts",
    "test:interop:ts-python": "vitest run --config vitest.interop.config.ts --reporter=verbose src/__tests__/interop/ts-python.test.ts",
    "test:interop:python-ts": "vitest run --config vitest.interop.config.ts --reporter=verbose src/__tests__/interop/python-ts.test.ts",
    "test:interop:a2a": "vitest run --config vitest.interop.config.ts --reporter=verbose --reporter=junit --outputFile.junit=test-results/a2a-junit.xml src/__tests__/interop/a2a.interop.test.ts",
    "test:interop:langgraph": "vitest run --config vitest.interop.config.ts --reporter=verbose --reporter=junit --outputFile.junit=test-results/langgraph-junit.xml src/__tests__/interop/langgraph.interop.test.ts",
    "test:interop:mcp": "vitest run --config vitest.interop.config.ts --reporter=verbose --reporter=junit --outputFile.junit=test-results/mcp-junit.xml src/__tests__/interop/mcp.interop.test.ts",
    "test:interop:python": "vitest run --config vitest.interop.config.ts --reporter=verbose src/__tests__/interop/ts-python.test.ts src/__tests__/interop/python-ts.test.ts",
    "prepublishOnly": "pnpm build && pnpm test",
    "publish:dry": "npm publish --dry-run",
    "publish": "npm publish --provenance --access public"
  },
  "dependencies": {
    "@ivxp/protocol": "workspace:*",
    "viem": "^2.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@ivxp/adapter-a2a": "workspace:*",
    "@ivxp/adapter-langgraph": "workspace:*",
    "@ivxp/adapter-mcp": "workspace:*",
    "@ivxp/test-utils": "workspace:*",
    "@a2a-js/sdk": "^0.3.0",
    "@langchain/langgraph": "^0.2.0",
    "@vitest/coverage-v8": "4.0.18",
    "vitest": "3.2.4",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "keywords": [
    "ivxp",
    "protocol",
    "sdk",
    "typescript",
    "web3",
    "blockchain",
    "usdc",
    "agent",
    "ai-agent",
    "base",
    "eip-191",
    "crypto",
    "payment",
    "p2p"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/franksprotocols/ivxp-protocol.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/franksprotocols/ivxp-protocol/issues"
  },
  "homepage": "https://github.com/franksprotocols/ivxp-protocol/tree/main/packages/sdk#readme",
  "license": "MIT"
}
